    "sass": "^1.69.5",
    "sass-loader": "^13.3.2"
  },
  "plugins": [
    "@babel/plugin-syntax-jsx"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest",
      "^.+\\.scss$": "jest-transform-stub"
